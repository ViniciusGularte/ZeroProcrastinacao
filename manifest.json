{
"manifest_version": 2,
    "name": "[ZeroProcrastinacão]",
    "description": "[Extensão para evitar ficar tempo demais em sites de procrastinação.]",
    "version": "0.0.1",
    "icons": {
     "48": "icon.png",
     "96": "icon@2x.png"
     },
     "sidebar_action": {
        "default_icon": {
          "16": "icon.png",
          "32": "icon.png"
        },
      "default_title": "ZeroProcrastinacão",
      "default_panel": "index.html",
      "open_at_install":true
    },
    "homepage_url": "https://github.com/ViniciusGularte/ZeroProcrastinacao",
    "content_scripts": [
        {
      "matches": ["<all_urls>"],
      "js": ["filter.js"]
    }
  ]
}
