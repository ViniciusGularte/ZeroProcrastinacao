{
"manifest_version": 2,
    "name": "[ZeroProcrastinacão]",
    "description": "[Extensão para evitar ficar tempo demais em sites de procrastinação.]",
    "version": "0.0.1",
    "icons": {
     "48": "icons/icon.png",
     "96": "icons/icon@2x.png"
     },
    "permissions": [
    "activeTab"
     ],
       "browser_action": {
         "default_popup": "popup/popup.html",
         "default_title":"ZeroProcrastinacao",
         "default_icon": {
         "19": "icons/icon.png",
         "38": "icons/icon.png"
       },
       "default_title": "ZeroProcrastinação"
     },
    "homepage_url": "https://github.com/ViniciusGularte/ZeroProcrastinacao",
    "content_scripts": [
        {
      "matches": ["<all_urls>"],
      "js": ["filter.js"]
    }
  ]
}
